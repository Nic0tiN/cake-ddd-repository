{
    "name": "ngirardet/cake-ddd-repository",
    "description": "CakePHP Domain Driven Design Rest Repository",
    "homepage": "https://github.com/Nic0tiN/cake-ddd-repository",
    "type": "library",
    "authors": [
        {
            "name": "N. Girardet",
            "homepage": "https://github.com/Nic0tiN"
        }
    ],
    "license": "MIT",
    "minimum-stability": "stable",
    "require": {
        "php": "^8.1",
        "cakephp/cakephp": "^4.5|^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "CakeDDD\\Repository\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CakeDDD\\Repository\\Test\\": "tests/"
        }
    },
    "suggest": {
        "nic0tin/cake-ddd-controller": "CakePHP Domain Driven Design Building Blocks - REST Controller"
    },
    "scripts": {
        "test": "./vendor/bin/phpunit --bootstrap ./tests/bootstrap.php ./tests"
    },
    "prefer-stable": true
}
